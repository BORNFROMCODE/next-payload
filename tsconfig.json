{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "rootDir": "./",
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "paths": {
      "payload/generated-types": [
        "./payload-types.ts"
      ],
      "@payloadcms/next-payload/*": [
        "./*"
      ]
    },
    "noEmit": true
  },
}
